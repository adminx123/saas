{
    "name": "my-web-app",
    "version": "1.0.0",
    "description": "A web application project.",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --mode development",
        "build": "webpack --mode production && npm run embed-schema",
        "embed-schema": "node src/database/schema/embed-schema.js",
        "test": "jest",
        "login": "npx wrangler login",
        "deploy:orchestrator": "npx wrangler deploy --config src/orchestrator/wrangler-orchestrator.toml",
        "deploy:widget": "npx wrangler pages deploy src/widget --project-name=master-widget",
        "deploy:inexasli": "npx wrangler deploy --config client/inexasli/config-inexasli.toml",
        "deploy:meofia": "npx wrangler deploy --config client/meofia/social-meofia.toml --env meofia",
        "deploy:payment": "npx wrangler deploy --config src/payment/stripe-saas.toml"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@langchain/community": "^1.0.0",
        "@langchain/core": "^1.0.2",
        "@langchain/groq": "^1.0.0",
        "@langchain/openai": "^1.0.0",
        "@modelcontextprotocol/sdk": "^1.21.0",
        "html5-qrcode": "^2.3.8",
        "langchain": "^1.0.2",
        "webpack": "^5.0.0",
        "webpack-cli": "^4.0.0",
        "webpack-dev-server": "^3.0.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "babel-loader": "^8.0.0",
        "css-loader": "^5.0.0",
        "jest": "^26.0.0",
        "style-loader": "^2.0.0",
        "wrangler": "^4.45.3"
    }
}
